// Place your settings in this file to overwrite default and user settings.
{
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.DS_Store": true,
        "**/*.js": {"when": "$(basename).ts"},
        "**/*.css": {"when": "$(basename).less"},
        "**/*.js.map": true,
        "app/**/*.js": true,
        "app/**/*.css": true,
        "tscommand*.txt": true,
        "*.csproj.user": true,
        ".tscache/": true
    }
}